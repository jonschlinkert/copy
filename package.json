{
  "name": "copy",
  "description": "Copy files or directories using globs.",
  "version": "0.2.0",
  "homepage": "https://github.com/jonschlinkert/copy",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/copy",
  "bugs": {
    "url": "https://github.com/jonschlinkert/copy/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "lib/"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "async": "1.5.2",
    "bluebird": "^3.3.4",
    "extend-shallow": "^2.0.1",
    "file-contents": "^0.2.4",
    "glob-parent": "^2.0.0",
    "graceful-fs": "^4.1.3",
    "has-glob": "^0.1.1",
    "lazy-cache": "^1.0.3",
    "matched": "^0.4.1",
    "mkdirp": "^0.5.1",
    "resolve-dir": "^0.1.0",
    "to-file": "^0.1.7",
    "vinyl": "^1.1.1"
  },
  "devDependencies": {
    "assert-fs": "^0.1.0",
    "assert-path": "^0.1.0",
    "delete": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-format-md": "^0.1.7",
    "look-up": "^0.8.3",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "async",
    "copy",
    "file",
    "file-system",
    "fs",
    "glob",
    "move",
    "path",
    "pattern",
    "promise",
    "stream",
    "sync",
    "system"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "write",
        "expand-config",
        "expand-target",
        "expand-task",
        "expand-files",
        "export-files"
      ]
    },
    "reflinks": [
      "gulp",
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
